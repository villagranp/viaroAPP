@model ViaroAPP.Models.Course

@{
    ViewBag.Title = "Detalle";
}

<h2>Detalle</h2>

<div>
    <h4>Curso</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.name)
        </dd>

        <dt>
            Profesor:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.teacher.first_name)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id_course }) |
    @Html.ActionLink("Regresar", "Index")
</p>
